{
  "extends": "../tslint.json",
  "rules": {
    "no-trailing-whitespace": false,
    "no-bitwise": false,
    "curly": [true, "as-needed"],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
